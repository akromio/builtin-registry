{
  "name": "{{pluginName}}",
  "version": "0.0.0",
  "description": "{{pluginDesc}}",
  "author": {
    "name": "{{authorName}}",
    "email": "{{authorEmail}}",
    "url": "{{authorUrl}}"
  },
  "keywords": [
    "akromio",
    "plugin"
  ],
  "bugs": {
    "url": "https://github.com/{{gitOwner}}/{{gitRepo}}/issues",
    "email": "{{bugsEmail}}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/{{gitOwner}}/{{gitRepo}}.git"
  },
  "preferGlobal": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">= 14",
    "npm": ">= 6"
  },
  "dependencies": {
    "@dogmalang/core": "^1.0.0-rc18.0"
  },
  "devDependencies": {
    "@akromio/doubles": "^0.4.0",
    "@akromio/expected": "^0.5.1",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "c8": "^7.12.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1"
  },
  "scripts": {
    "lint": "dogmac check src tests",
    "build": "npm run build/src",
    "build/src": "rm -rf dist/ && dogmac js -o dist/cjs src && babel -d dist dist",
    "test": "npm run test/build && mocha --config .mocharc.yaml",
    "test/build": "rm -rf ./__tests__ && dogmac js -o __tests__ tests && babel -d __tests__ __tests__",
    "cov": "c8 mocha --config .mocharc.yaml"
  }
}
